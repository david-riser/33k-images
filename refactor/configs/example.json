{
    "exp": {
	"name": "example_config"
    },
    "data_loader": {
	"name": "data_loader.InMemoryDataLoader",
	"base_dir": "/home/ubuntu/data",
	"batch_size": 32,
	"min_samples": 320 
    },
    "model":{
	"name": "pretrained_model.PretrainedLinearModel",
	"learning_rate": 0.001,
	"optimizer": "adam",
	"backbone": "ResNet50",
	"pooling": "avg"
    },
    "trainer":{
	"name": "supervised_trainer.SupervisedTrainer",
	"num_epochs": 2,
	"verbose_training": true
    },
    "callbacks":{
	"checkpoint_monitor": "val_loss",
	"checkpoint_mode": "min",
	"checkpoint_save_best_only": true,
	"checkpoint_save_weights_only": true,
	"checkpoint_verbose": true,
	"checkpoint_dir": "/home/ubuntu/checkpoints/"
    },
    "evaluators":[
	{
	    "name": "plot_evaluators.GridPlottingEvaluator",
	    "output_name": "/home/ubuntu/grid_plot_eval_images.pdf",
	    "nrows": 6,
	    "ncols": 5,
	    "samples": 320,
	    "dpi": 100
	}
    ]
}
