{
    "exp": {
	"name": "example_config"
    },
    "data_loader": {
	"name": "data_loader.InMemoryDataLoader",
	"base_dir": "/home/ubuntu/data",
	"batch_size": 32,
	"min_samples": 320,
	"preprocessing_func":"resnet50.preprocess_input",
	"augmentations":{
	    "horizontal_flip":true,
	    "zoom_range":0.2,
	    "rotation_range":20,
	    "height_shift_range":0.1,
	    "width_shift_range":0.1
	}
    },
    "model":{
	"name": "pretrained_model.PretrainedModel",
	"backbone": "ResNet50",
	"pooling": "avg",
	"dense_neurons": 32,
	"dense_activation": "relu",
	"dropout":0.35,
	"optimizer": {
	    "name":"adam",
	    "params":{
		"learning_rate":0.0001,
		"beta_1": 0.9,
		"beta_2": 0.99
	    }
	}
    },
    "trainer":{
	"name": "supervised_trainer.SupervisedTrainer",
	"num_epochs": 6,
	"verbose_training": true,
	"steps_per_epoch": 50
    },
    "callbacks":{
	"checkpoint_monitor": "val_loss",
	"checkpoint_mode": "min",
	"checkpoint_save_best_only": true,
	"checkpoint_save_weights_only": true,
	"checkpoint_verbose": true,
	"checkpoint_dir": "/home/ubuntu/checkpoints/"
    },
    "evaluators":[
	{
	    "name": "plot_evaluators.GridPlottingEvaluator",
	    "output_name": "/home/ubuntu/grid_plot_eval_images.pdf",
	    "nrows": 6,
	    "ncols": 5,
	    "samples": 300,
	    "dpi": 100
	}
    ]
}
